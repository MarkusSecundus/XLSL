struct Color {
  number r;
  number g;
  number b;
  serialize {
    return "A";
  }
  deserialize {
    return Color { 1, 2, 3 };
  }
}

Color getColor() {
  number rValue = 102; // Rebecca purple
  number gValue = 51;
  number bValue = 153;
  return Color { rValue, gValue, bValue };
}

number main() {
  Color color = getColor();
  return color.r;
}